#iv_home_footer_inner {
  padding: 14px !important;
  background-color: white;
}

.global #content {
  overflow: visible !important;
}

#content .col-636-300-wrap:first-child {
  display: none;
}

.b-view_content {
  position: relative;
  background-color: #F3F3F3;
  padding-top: 10px;
  padding-bottom: 20px;
  border-bottom: 1px solid #CCC;
}

.b-facebook .b-view_content {
  background-color: transparent;
  border-bottom: 0 none;
}

.b-view_aside {
  float: left;
  width: 319px;
  padding: 0px 0px 20px 9px;
  border-left: 1px solid #DDD;
}

.b-view_container .b-info{
  padding: 0 28px;
  display: none;
}

.b-view_container #carousel {
  width: 482px;
  height: 372px;
  padding: 0px 18px;
  position: relative;
  overflow: hidden;
}

.b-slide, .b-slides {
  background: url("<%= asset_path 'slides/loading.gif' %>") no-repeat -9999px -9999px;
}

.b-facebook .b-slide, .b-facebook .b-slides {
  width: 670px;
  margin: 0 auto;
}

.b-top_banner {
  display: block;
  left: 50%;
  margin-left: -510px;
  position: relative;
  width: 1020px;
}

.b-top_banner img {
  display: block;
  margin: 0 auto;
}

h2.b-view_heading {
  font-size: 22px;
  padding: 12px 0;
  font-style: italic;
  font-weight: bold;
  font-family: Georgia, "Helvetica Neue",Helvetica,Arial,sans-serif;
}

.b-view_heading.profile {
  text-align: center;
  font-size: 24px;
}

.b-info_entryname {
  font-weight: bold;
}

h3.b-view_heading {
  font-size: 14px;
  line-height: 26px;
  padding-top: 10px;
  padding-bottom: 10px;
  margin-left: 5px;
  font-weight: bold;
}

h3.b-view_heading_halloween-costumes {
    color: white !important;
}

h4.b-view_heading {
  font-size: 12px;
  line-height: 26px;
  padding-bottom: 5px;
  color: #999;
}

p.b-view_heading {
  color: #666666;
  font-size: 13px;
  padding-bottom: 16px;
  width: 626px;
}

.b-view_heading {
  margin-left: 10px;
}

.b-view_container h2, .b-view_container h3{
  font-family: Georgia, "Helvetica Neue",Helvetica,Arial,sans-serif;
  font-style: italic;
}

.b-view_container h2{
  font-size: 24px;
  line-height: 36px;
  padding-bottom: 16px;
}

.b-facebook .b-view_container h2{
  padding-bottom: 0;
  padding-top: 26px;
}

.b-view_container h3{
  font-size: 18px;
  line-height: 27px;
  padding-bottom: 10px;
}

.b-view_container {
  background-color: white;
}

.b-facebook .b-view_container {
  background-color: transparent;
}

.b-view_wrapper {
  border-top: 3px double #AAA;
  border-bottom: 3px double #AAA;
}

.b-view_wrapper_inner {
  padding: 20px 8px;
}

.b-view_header, .b-view_footer {
  width: 1000px;
  left: 50%;
  margin-left: -501px;
  position: relative;
}

.b-view_container:before, .b-view_container:after {
  display: table;
  content: " ";
}

.b-view_container:after {
  clear: both;
}

.b-view_container .b-info_entry_id {
  display: none;
}

.b-view_container .b-info_user {
  width: 200px;
  background: white;
  padding: 4px;
}

.b-view_container img.b-info_user-profile{
  vertical-align: middle;
  width: 48px;
  height: 48px;
}

.b-view_container span.b-info_username{
  padding-left: 6px;
  text-transform: capitalize;
}

.b-view_container p.b-info_description{
  padding-bottom: 20px;
}

.b-view_container .b-info_user-info p{
  padding-bottom: 4px;
}

.jcarousel-skin-tango {
  width: 518px;
  float: left;
}

div[class*='b-slides_social']  {
  display: none;
  float: left;
  width: 90px;
  padding-top: 10px;
  text-align: center;
  height: 348px;
  position: relative;
}

div[class*='b-slides_social'] a img{
  margin: 0 5px;
}

#b-gigya {
  height: 20px;
  padding-left: 8px;
  padding-bottom: 16px;
}

.b-gigya-vertical .gig-bar-container *, div.gig-reactions-container, .gig-reactions-container span, .gig-reactions-container a:hover, .gig-reactions-container a:visited, .gig-reactions-container a:link, .gig-reactions-container a:active,
#b-gigya-vertical .gig-bar-container *, div.gig-reactions-container, .gig-reactions-container span, .gig-reactions-container a:hover, .gig-reactions-container a:visited, .gig-reactions-container a:link, .gig-reactions-container a:active {
  margin: 0 auto !important;
}
.b-gigya-verticall .gig-bar-container *, div.gig-reactions-container, .gig-reactions-container span, .gig-reactions-container a:hover, .gig-reactions-container a:visited, .gig-reactions-container a:link, .gig-reactions-container a:active,
#b-gigya-verticall .gig-bar-container *, div.gig-reactions-container, .gig-reactions-container span, .gig-reactions-container a:hover, .gig-reactions-container a:visited, .gig-reactions-container a:link, .gig-reactions-container a:active {
  margin: 0 auto !important;
}

div.gig-button-container-vertical {
  padding: 0 0 10px 0 !important;
}

.b-slides_rating span{
  display: inline-block;
  font-size: 18px;
  font-style: italic;
  font-weight: bold;
  padding-bottom: 7px;
  padding-top: 12px;
}

.b-slides_rate_loader{
  display: none;
  padding: 5px 0;
  width: 90px;
}

.b-slides_rate_loader.gigya_loader_show{
  position: absolute;
  bottom: 0;
  display: block;
}

.b-slides_rate_loader img{
  display: block;
  margin: 0 auto;
}

.b-view_wrapper .vote {
    font-size: 18px;
    font-weight: bold;
}

div[class*='b-slides_social'] span.rate_up{
  color: #42B53F;
  padding-right: 6px;
}

div[class*='b-slides_social'] span.rate_down{
  color: #FF3535;
}

.b-slides_change-view {
  display: inline-block;
  margin: 0 0 10px 30px;
}

.needed .b-slides_change-view {
  margin: 0 0 10px 0px;
}

.b-slides_text {
  font-size: 13px;
  line-height: 16px;
  padding: 40px 0 20px;
  text-align: left;
}

#b-slides_share, .b-gigya-vertical  {
  margin-left: -14px;
  position: absolute;
  bottom: 0;
}

#b-slides_share .gig-share-bar-container *,
.b-gigya-vertical .gig-share-bar-container * {
  background: none !important;

}

#b-slides_share .gig-share-bar-container div.gig-button-text,
.b-gigya-vertical .gig-share-bar-container div.gig-button-text {
 text-indent: -9999px;
  font-size: 0;
  line-height: 0;
}

#b-slides_share .gig-share-bar-container .gig-button-td,
.b-gigya-vertical .gig-share-bar-container .gig-button-td {
  background: url("slides/share-entry.png") no-repeat 0 0 !important;
  width: 117px;
  height: 24px;
}

#b-slides_share div.gig-button-container,
.b-gigya-vertical div.gig-button-container {
  width: 117px;
}

#b-slides_question {
  display: none;
}